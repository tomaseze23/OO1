<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Lote" table="Lote">
		<id column="idLote" name="idLote">
			<generator class="identity" />
		</id>
		<property column="fecha" name="fecha" type="LocalDate" />
		<property column="cantExistente" name="cantExistente"
			type="int" />
		<property column="cantInicial" name="cantInicial" type="int" />
	</class>
</hibernate-mapping>
