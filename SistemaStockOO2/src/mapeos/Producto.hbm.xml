<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="datos.Producto" table="Producto">
    <id column="idProducto" name="idProducto">
      <generator class="identity"/>
    </id>
     <property column="codigo" name="codigo" type="string"/>
     <property column="producto" name="producto" type="string"/>
     <property column="precio" name="precio" type="double"/>
</class>    
</hibernate-mapping>
