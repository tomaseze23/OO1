<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="datos.Espacio" table="Espacio">
    <id column="idEspacio" name="idEspacio">
      <generator class="identity"/>
    </id>
     <property name="fecha" column="fecha" type="LocalDate"/>
     <property name="turno" column="turno" type="char"/>
     <property name="libre" column="libre" type="boolean"/>
     
    <many-to-one name="aula" class="datos.Aula" column="idAula" not-null="true" />
     
</class>    
</hibernate-mapping>